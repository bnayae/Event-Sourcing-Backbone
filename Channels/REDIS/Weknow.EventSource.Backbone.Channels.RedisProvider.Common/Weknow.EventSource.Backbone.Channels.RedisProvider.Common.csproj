<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Weknow</Authors>
    <Company>Weknow</Company>
  </PropertyGroup>
  
  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
  </PropertyGroup>
  <PropertyGroup>
    <DocumentationFile>Weknow.EventSource.Backbone.Channels.RedisProvider.Common.xml</DocumentationFile>
    <PackageProjectUrl>https://github.com/weknow-network/Event-Source-Backbone</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/weknow-network/Event-Source-Backbone</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Weknow.EventSource.Backbone.Channels.RedisProvider.Common.xml" />
  </ItemGroup>
  <ItemGroup Condition="'$(Configuration)'!='ProjRef'">
    <PackageReference Include="Weknow.EventSource.Backbone.Contracts" Version="1.0.1" />
  </ItemGroup>
  <ItemGroup Condition="'$(Configuration)'=='ProjRef'">
    <ProjectReference Include="..\..\..\Weknow.EventSource.Backbone.Contracts\Weknow.EventSource.Backbone.Contracts.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="StackExchange.Redis" Version="2.1.58" />
  </ItemGroup>
  <ItemGroup>
    <None Include="icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
