<Project>
	<PropertyGroup>
		<Version>1.1.139</Version>
		<PackageReleaseNotes>
			BREAKING CHANGES: rename of expected environment variables:
			REDIS_EVENT_STREAM_ENDPOINT => REDIS_EVENT_SOURCE_ENDPOINT
			REDIS_EVENT_STREAM_PASS => REDIS_EVENT_SOURCE_PASS
			S3_ACCESS_KEY => S3_EVENT_SOURCE_ACCESS_KEY
			S3_SECRET => S3_EVENT_SOURCE_SECRET
			S3_REGION => S3_EVENT_SOURCE_REGION
		</PackageReleaseNotes>
	</PropertyGroup>
	
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<LangVersion>10</LangVersion>
		<Nullable>enable</Nullable>
		<Configurations>Debug;Release;Gen</Configurations>
		<ImplicitUsings>enable</ImplicitUsings>
		<PackageIcon>icon.png</PackageIcon>
	</PropertyGroup>

	<PropertyGroup>
		<PackageTags>event-source, pub, sub, pubsub, messaging, reliable, redis, weknow, bnaya</PackageTags>
		<PackageProjectUrl>https://medium.com/weknow-network</PackageProjectUrl>
		<RepositoryUrl>https://github.com/weknow-network/Event-Source-Backbone</RepositoryUrl>
		<RepositoryType>GitHub</RepositoryType>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

	<PropertyGroup>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<IncludeSource>True</IncludeSource>
		<IncludeSymbols>True</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
	</PropertyGroup>

	<PropertyGroup>
		<PackageReleaseNotes>[BREAKING CHANGES] REDIS segmentation key have different pattern (not compatible with previous pattern)</PackageReleaseNotes>
	</PropertyGroup>

	<PropertyGroup>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NoWarn>$(NoWarn);1591</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<None Include="icon.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>
</Project>